{
  "name": "@claudetree/cli",
  "version": "0.4.5",
  "description": "CLI for claudetree - Manage Claude sessions with git worktrees",
  "type": "module",
  "bin": {
    "claudetree": "./dist/index.js",
    "ct": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "claude",
    "cli",
    "git",
    "worktree",
    "ai",
    "session"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wonjangcloud9/claude-tree.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/wonjangcloud9/claude-tree#readme",
  "license": "MIT",
  "dependencies": {
    "@claudetree/core": "workspace:*",
    "@claudetree/shared": "workspace:*",
    "async-mutex": "^0.5.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
